<Application x:Class="Path_Finding_Visualizer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Path_Finding_Visualizer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="GridStrokeBrush" Color="#949b9b"/>
        <SolidColorBrush x:Key="PointNoneBrush" Color="White"/>
        <SolidColorBrush x:Key="PointExploredBrush" Color="#29e2ea"/>
        <SolidColorBrush x:Key="PointShortestPathBrush" Color="#bb54e3"/>
        <SolidColorBrush x:Key="PointBorderBrush" Color="#3c3c3c"/>
    </Application.Resources>
</Application>
